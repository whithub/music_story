<section class="header">
  <div class="name-title">
    <ul><img class="img-rounded" <%= image_tag(@artist_images.first.url, :class => 'list_image') %></ul>
    <ul><h1 class="artist-name"><%= @artist.name %></h1></ul>
    <% if current_user.likes.where(artist_name: @artist.name).count > 0 %>
      <p>Already Liked</p>
    <% else %>
      <%= render partial: 'like_form', locals: {artist_name: @artist.name, found_through: params[:found_through]} %>
    <% end %>
    <%= link_to "Find similiar artists to #{@artist.name}", artist_similar_artists_path(@artist.name) %>
  </div>

</section>

